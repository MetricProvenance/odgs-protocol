    <!-- CLASSES — The 6 Domain Types of the Constitutional Stack            -->
    <!-- ================================================================== -->
    <owl:Class rdf:about="&odgs;Metric">
        <rdfs:label xml:lang="en">Metric</rdfs:label>
        <rdfs:comment xml:lang="en">A quantitative business measure. Lives in the Legislative Plane.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&odgs;"/>
    </owl:Class>

    <owl:Class rdf:about="&odgs;Rule">
        <rdfs:label xml:lang="en">Data Governance Rule</rdfs:label>
        <rdfs:comment xml:lang="en">An enforceable data validation rule. Lives in the Judiciary Plane.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&odgs;"/>
    </owl:Class>

    <owl:Class rdf:about="&odgs;Process">
        <rdfs:label xml:lang="en">Business Process Stage</rdfs:label>
        <rdfs:comment xml:lang="en">A stage in a business process lifecycle. Lives in the Executive Plane.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&odgs;"/>
    </owl:Class>

    <owl:Class rdf:about="&odgs;Factor">
        <rdfs:label xml:lang="en">Root Cause Factor</rdfs:label>
        <rdfs:comment xml:lang="en">An environmental factor causing data quality issues.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&odgs;"/>
    </owl:Class>

    <owl:Class rdf:about="&odgs;Dimension">
