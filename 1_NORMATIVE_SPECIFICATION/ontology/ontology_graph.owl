    <owl:Ontology rdf:about="&odgs;">
        <dcterms:title xml:lang="en">Open Data Governance Standard Ontology</dcterms:title>
        <dcterms:description xml:lang="en">
            Formal ontology for the ODGS Constitutional Stack. Defines the semantic
            relationships between Metrics, Rules, Processes, Factors, Dimensions,
            and Sovereign Definitions in a 5-Plane governance architecture.
            Contains 72 Metrics, 50 Rules, 60 Dimensions,
            8 Factors, 42 Process Stages,
            and 43 Sovereign Definitions from 5 regulatory domains.
        </dcterms:description>
        <dcterms:creator>Kartik Iyer</dcterms:creator>
        <dcterms:license rdf:resource="https://www.apache.org/licenses/LICENSE-2.0"/>
        <owl:versionInfo>3.3.0</owl:versionInfo>
        <dcterms:conformsTo>EU AI Act (2024/1689) Articles 10, 12</dcterms:conformsTo>
        <dcterms:conformsTo>ISO/IEC 42001:2023</dcterms:conformsTo>
        <dcterms:conformsTo>NEN 381 525</dcterms:conformsTo>
    </owl:Ontology>

